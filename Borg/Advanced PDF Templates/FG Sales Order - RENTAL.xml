    </macrolist>
    <style type="text/css">
      table {
    <#if .locale == "zh_CN">
        font-family: stsong, sans-serif;
    <#elseif .locale == "zh_TW">
        font-family: msung, sans-serif;
    <#elseif .locale == "ja_JP">
        font-family: heiseimin, sans-serif;
    <#elseif .locale == "ko_KR">
        font-family: hygothic, sans-serif;
    <#elseif .locale == "ru_RU">
        font-family: verdana;
    <#else>
        font-family: sans-serif;
    </#if>
        font-size: 9pt;
        table-layout: fixed;
    }
    th {
        font-weight: bold;
        font-size: 8pt;
        vertical-align: middle;
        padding: 5px 6px 3px;
        background-color: #e3e3e3;
        color: #333333;
    }
    td {
        padding: 4px 6px;
        border: 0px solid #efefef;
    }
    b {
        font-weight: bold;
        color: #333333;
    }
    table.header td {
        padding: 0;
        font-size: 10pt;
    }
    table.footer td {
        padding: 0;
        font-size: 8pt;
    }
    table.itemtable th {
        padding-bottom: 10px;
        padding-top: 10px;
    }
    table.body td {
        padding-top: 2px;
    }
    table.total {
        page-break-inside: avoid;
    }
    tr.totalrow {
        background-color: #e3e3e3;
        line-height: 200%;
    }
    td.totalboxtop {
        font-size: 12pt;
        background-color: #e3e3e3;
    }
    td.addressheader {
        font-size: 8pt;
        padding-top: 6px;
        padding-bottom: 2px;
    }
    td.address {
        padding-top: 0;
    }
    td.totalboxmid {
        font-size: 28pt;
        padding-top: 20px;
        background-color: #e3e3e3;
    }
    td.totalboxbot {
        background-color: #e3e3e3;
        font-weight: bold;
    }
    span.title {
        font-size: 28pt;
    }
    span.number {
        font-size: 16pt;
    }
    span.itemname {
        font-weight: bold;
        line-height: 150%;
    }
    hr {
        width: 100%;
        color: #d3d3d3;
        background-color: #d3d3d3;
        height: 1px;
    }
    a {
      color: #d41400;
      text-decoration: none;
    }
    h3 {
    font-style: normal;
    text-transform: none;
    font-family: "Arial", sans-serif;
    }
    span.small-heading {
        color: #A8A8A8!important;
    }
      tr.removed_item {
        /*background-color: #e3e3e3;*/
	}
      tr.new_item {
        background-color: #A8A8A8;
      }
</style>
</head>
<body header="nlheader" header-height="200" footer="nlfooter" footer-height="30pt" padding="15mm 15mm 15mm 15mm" size="A4">
<table margin-left="-10" style="width: 100%; margin-top: 10px;">
  	<tr>
		<td border-left="1" padding-left="10" valign="top"><span class="small-heading">INVOICE ADDRESS</span></td>
		<td border-left="1" padding-left="10" valign="top"><span class="small-heading">DELIVERY ADDRESS</span></td>
	</tr>
	<tr>
		<td border-left="1" padding-left="10" valign="top" width="50%">
          	<p>
            ${record.billaddressee?capitalize}<br/>
            ${record.billaddr1?capitalize}<br/>
            <#if record.billaddr2 !="">${record.billaddr2?capitalize}<br/><#else> </#if>
            <#if record.billaddr3 !="">${record.billaddr3?capitalize}<br/><#else> </#if>
            ${record.billcity?capitalize}, ${record.billstate}<br/>
            ${record.billzip?upper_case}
            </p>
		</td>
            <td border-left="1" padding-left="10" valign="top" width="50%">
			<p>${record.billaddressee?capitalize}<br/>
            ${record.shipaddr1?capitalize}<br/>
            <#if record.shipaddr2 !="">${record.shipaddr2?capitalize}<br/><#else> </#if>
            <#if record.shipaddr3 !="">${record.shipaddr3?capitalize}<br/><#else> </#if>
            ${record.shipcity?capitalize} ${record.shipstate}<br/>
            ${record.shipzip?upper_case}
            </p>
		</td>
	</tr>
</table>
<!--
<table style="width: 100%; margin-top: 10px;">
  <tr height="20">
  	<td>&nbsp;</td>
  </tr>
  <tr>
	<th>${record.terms@label}</th>
	<th>${record.otherrefnum@label}</th>
	<th>${record.shipmethod@label}</th>
	<th>${record.shipdate@label}</th>
	</tr>
	<tr>
	<td style="padding-top: 2px;">${record.terms}</td>
	<td style="padding-top: 2px;">${record.otherrefnum}</td>
	<td style="padding-top: 2px;">${record.shipmethod}</td>
	<td style="padding-top: 2px;">${record.shipdate}</td>
	</tr>
  </table>
-->
<#if record.item?has_content>

<table style="width: 100%; margin-top: 10px;"><!-- start items -->
  <#list record.item as item>
    <#if item_index==0>
	<thead>
      <tr width="100%">
        <th align="center" height="24px" colspan="2">Qty</th>
        <th align="left" colspan="10">${item.item@label}</th>
        <th align="center" colspan="4">Billing Period (Wks)</th>
        <th align="right" colspan="4">${item.rate@label}</th>
        <th align="right" colspan="4">${item.amount@label}</th>
      </tr>
      <tr width="100%">
      		<td align="left" colspan="24" line-height="150%">&nbsp;</td>
		</tr>
	</thead>
	</#if>
   
      <#if item.itemtype = "Description">
      <tr class="desc_line" >
      	<td colspan="24">
        	<span>${item.item} <b>${item.custcol_fg_fw_site_num}</b> (${item.custcol_fg_imp_loc})</span> From ${record.custbody_dkdrb_currentperiodstart} to ${record.custbody_dkdrb_currentperiodend}
        </td>
      </tr>
    </#if>
    <#if item.itemtype != "Description">
        <#if item.fulfillable = "T"> <!--SALE/EXTRA ITEMS-->
          <tr class="new_item">
            <td align="center" colspan="2" line-height="150%">${item.quantity}</td>
            <td colspan="10">
              <span style="font-weight: bold; line-height: 150%; color: #333333;">${item.item}</span> ${item.description}
            </td>
            <td align="center" colspan="4" line-height="150%">&nbsp;</td>
            <td align="right" colspan="4">${item.rate}</td>
            <td align="right" colspan="4">${item.amount}</td>
          </tr>       
       
       <#else>
          <tr>
            <td align="center" valign="top" colspan="2" line-height="150%">${item.custcol_dkdrb_unitqty}</td>
            <td colspan="10">
              <span style="font-weight: bold; line-height: 150%; color: #333333;">${item.item}</span> ${item.description}<br/>
              
              <span>From ${item.custcol_dkdrb_startdate} 
                to <#if item.custcol_dkdrb_enddate?has_content>${item.custcol_dkdrb_enddate}<#else> ${record.custbody_dkdrb_currentperiodend}</#if> </span> <br/>
              
              <span>Location: ${item.custcol_fajob_field_location}</span>
            </td>
            <td align="center" colspan="4" line-height="150%">${item.quantity}</td>
            <td align="right" colspan="4">${item.rate}</td>
            <td align="right" colspan="4">${item.amount}</td>
          </tr>
    	</#if>
     </#if>
      </#list><!-- end items --></table>

<hr style="width: 100%; color: #d3d3d3; background-color: #d3d3d3; height: 1px;" /></#if>
<table style="page-break-inside: avoid; width: 100%; margin-top: 10px;"><tr>
	<td colspan="4">&nbsp;</td>
	<td align="right" style="font-weight: bold; color: #333333;">${record.subtotal@label}</td>
	<td align="right">${record.subtotal}</td>
	</tr>
	<tr>
	<td colspan="4">&nbsp;</td>
	<td align="right" style="font-weight: bold; color: #333333;">${record.taxtotal@label} (${record.taxrate}%)</td>
	<td align="right">${record.taxtotal}</td>
	</tr>
	<tr style="background-color: #e3e3e3; line-height: 200%;">
	<td background-color="#ffffff" colspan="4">&nbsp;</td>
	<td align="right" style="font-weight: bold; color: #333333;">${record.total@label}</td>
	<td align="right">${record.total}</td>
	</tr></table>
</body>
</pdf>