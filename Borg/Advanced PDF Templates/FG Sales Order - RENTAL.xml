      <table width="100%" >
                  <tr width="100%">
                    <td align="left" style="padding: 0px;">
                     Page1
                    </td>
                </tr>
              </table>
    </macro>
    <macro id="nlnotpage1footer">
      <table width="100%" >
                  <tr width="100%">
                    <td align="left" style="padding: 0px;">
                     Not Page1
                    </td>
