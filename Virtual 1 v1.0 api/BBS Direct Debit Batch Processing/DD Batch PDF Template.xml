	<link name="OpenSans"   type="font" subtype="opentype" src="https://5117415-sb1.app.netsuite.com/core/media/media.nl?id=31190&amp;c=5117415_SB1&amp;h=b36a1bf1dc7c4e9792b5&amp;_xt=.ttf" bytes="2" />
	
	<#assign todaysDate = .now>
	
	<macrolist>
