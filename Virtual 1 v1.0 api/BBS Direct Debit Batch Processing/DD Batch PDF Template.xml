<body header="nlheader" header-height="300px"  footer="nlfooter" footer-height="30px" padding="1.5cm 1.5cm 1.5cm 1.5cm" size="A4">

<#if lines?has_content>

	<!-- Details table -->
	<table style="width: 100%;">	
		<#list lines as line>
			<#if line_index==0>
				<thead>
					<tr>
						<th align="left"  style="font-family: Montserrat; font-size: 7pt; color: #1d1b45; font-weight: bold; border-bottom: 1px; border-color: #ee2d4b;">Document<br/>Date</th>
						<th align="left"  style="font-family: Montserrat; font-size: 7pt; color: #1d1b45; font-weight: bold; border-bottom: 1px; border-color: #ee2d4b;">Document<br/>Reference</th>
						<th align="right" style="font-family: Montserrat; font-size: 7pt; color: #1d1b45; font-weight: bold; border-bottom: 1px; border-color: #ee2d4b;">Document<br/>Value</th>
						<th align="right" style="font-family: Montserrat; font-size: 7pt; color: #1d1b45; font-weight: bold; border-bottom: 1px; border-color: #ee2d4b;">Outstanding<br/>Amount</th>
						<th align="right" style="font-family: Montserrat; font-size: 7pt; color: #1d1b45; font-weight: bold; border-bottom: 1px; border-color: #ee2d4b;">In<br/>Query</th>
						<th align="right" style="font-family: Montserrat; font-size: 7pt; color: #1d1b45; font-weight: bold; border-bottom: 1px; border-color: #ee2d4b;">Collection<br/>Amount</th>
					</tr>
				</thead>
			</#if>
				
					<tr>
						<#if line.CUSTRECORD_BBS_DD_DET_PR.custrecord_bbs_presentation_record_date?length gt 0>
							<td align="left" style="font-family: OpenSans; font-size: 7pt; color: #393939;">${line.CUSTRECORD_BBS_DD_DET_PR.custrecord_bbs_presentation_record_date?string["dd/MM/yy"]}</td>
						<#else>
							<td align="left" style="font-family: OpenSans; font-size: 7pt; color: #393939;">&nbsp;</td>
						</#if>
						
						<td align="left"  style="font-family: OpenSans; font-size: 7pt; color: #393939;">${line.CUSTRECORD_BBS_DD_DET_PR.name}</td>
						<td align="right" style="font-family: OpenSans; font-size: 7pt; color: #393939;">${line.CUSTRECORD_BBS_DD_DET_PR.custrecord_bbs_pr_inv_total}</td>
						<td align="right" style="font-family: OpenSans; font-size: 7pt; color: #393939;">${line.CUSTRECORD_BBS_DD_DET_PR.custrecord_bbs_pr_inv_outstanding}</td>
						
						<#if line.CUSTRECORD_BBS_DD_DET_PR.custrecord_bbs_pr_inv_disputed gt 0>
							<td align="right" style="font-family: OpenSans; font-size: 7pt; color: #393939;">${line.CUSTRECORD_BBS_DD_DET_PR.custrecord_bbs_pr_inv_disputed}</td>
						<#else>
							<td align="right" style="font-family: OpenSans; font-size: 7pt; color: #393939;">&nbsp;</td>
						</#if>
						
						<td align="right" style="font-family: OpenSans; font-size: 7pt; color: #393939;">${line.custrecord_bbs_dd_det_amount}</td>
					</tr>
		</#list>
	</table>
	
	<div style="page-break-inside: avoid;">
		<!-- Totals table -->	
		<table style="width: 100%; margin-top: 10px; page-break-inside: avoid;">			
			<tr>
				<td align="left"  style="font-family: Montserrat; font-size: 7pt; color: #393939;">&nbsp;</td>
				<td align="left"  style="font-family: Montserrat; font-size: 7pt; color: #393939;">&nbsp;</td>
				<td align="right" style="font-family: Montserrat; font-size: 7pt; color: #393939;">&nbsp;</td>
				<td align="right" style="font-family: Montserrat; font-size: 7pt; color: #393939;">&nbsp;</td>
				<td align="right" style="font-family: Montserrat; font-size: 7pt; color: #1d1b45; font-weight: bold;" colspan="2">Total Collection Amount</td>
				<td align="right" style="font-family: Montserrat; font-size: 7pt; color: #1d1b45; font-weight: bold;">${record.custrecord_bbs_dd_amount_to_process}</td>
			</tr>
		</table>
	</div>
</#if>

</body>
</pdf>