                        <td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;">${toCurrency(expense.expenseAmount)}</td>
                    </tr>
                </#list>

                <tr>
                    <td align="left" 	colspan="18" style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF;"><b>Expenses Total</b></td>
                    <td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;"><b>${toCurrency(expenseTotal)}</b></td>
                </tr>

            </table>
	</#if>
</#if>

<!-- Item summary -->
<#if record.custbody_bbs_item_summary_json?length gt 0>

	<#assign summaryInfo = record.custbody_bbs_item_summary_json?eval>
    <#if summaryInfo?has_content>
        <#assign itemTotal = 0>

            <table style="width: 100%; margin-top: 10px; page-break-inside: avoid;">
                <tr>
                    <th align="left" 	colspan="21" style="font-size: 10pt; padding-left: 6px;"><b>Other Items Summary</b></th>
                </tr>
                <tr>
                    <th align="left" 	colspan="12" style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Item</th>
                    <th align="center"	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Hours</th>
                    <th align="center" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Rate</th>
                    <th align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;">Value</th>
                </tr>

                <#list summaryInfo as item>
                    <#assign itemTotal += item.itemAmount>
                    <tr>
                        <td align="left" 	colspan="12" style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">${item.itemItemName}</td>
                        <td align="right"	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;"><#if item.itemQuantity != 0>${item.itemQuantity}<#else>&nbsp;</#if></td>
                        <td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;"><#if item.itemRate != 0>${item.itemRate}<#else>&nbsp;</#if></td>
                        <td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;">${toCurrency(item.itemAmount)}</td>
                    </tr>
                </#list>

                <tr>
                    <td align="left" 	colspan="18" style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF;"><b>Item Total</b></td>
                    <td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;"><b>${toCurrency(itemTotal)}</b></td>
                </tr>

            </table>
	</#if>
</#if>

<!-- Totals -->
<table style="width: 100%; margin-top: 10px; page-break-inside: avoid;">


	<tr>
		<td align="left" 	colspan="12" rowspan="2">${record.message}</td>
		<td align="left" 	colspan="6"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Invoice Subtotal</td>
		<td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;">${toCurrency(record.subtotal)}</td>
	</tr>
	<tr>
		<td align="left" 	colspan="6"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF;">VAT (${record.taxrate}%)</td>
		<td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;">${toCurrency(record.taxtotal)}</td>
	</tr>
	<tr>
		<td align="left" 	colspan="12" >&nbsp;</td>
		<td align="left" 	colspan="6"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF;"><b>Invoice Total</b></td>
		<td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;"><b>${toCurrency(record.total)}</b></td>
	</tr>
	<tr>
		<td align="left" 	colspan="12" >&nbsp;</td>
		<td align="left" 	colspan="6"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF;">&nbsp;</td>
		<td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;">&nbsp;</td>
	</tr>
	<tr>
		<td align="left" 	colspan="12" >&nbsp;</td>
		<td align="left" 	colspan="6"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF;">Amount Paid</td>
