</style>
</head>

<!--Summary body section-->
<body header="nlheader" header-height="350px" footer="nlfooter" footer-height="50px" padding="0.25in 0.25in 0.25in 0.25in" size="A4">

<#setting number_format=",##0.00">

<!-- Time summary -->
<#if record.custbody_bbs_time_summary_json?length gt 0>

	<#assign summaryInfo = record.custbody_bbs_time_summary_json?eval>
    <#if summaryInfo?has_content>
        <#assign timeTotal = 0>

            <table style="width: 100%; page-break-inside: avoid;">
                <tr>
                    <th align="left" 	colspan="21" style="font-size: 10pt; padding-left: 6px;"><b>Time Summary</b></th>
                </tr>
                <tr>
                    <th align="left" 	colspan="12" style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Item</th>
                    <th align="center"	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Hours</th>
                    <th align="center" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Rate</th>
                    <th align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;">Value</th>
                </tr>

                <#list summaryInfo as time>
                    <#assign timeTotal += time.timeAmount>
                    <tr>
                        <td align="left" 	colspan="12" style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">${time.timeItemName}</td>
                        <td align="right"	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">${time.timeQuantity}</td>
                        <td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">${toCurrency(time.timeRate)}</td>
                        <td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;">${toCurrency(time.timeAmount)}</td>
                    </tr>
                </#list>

                <tr>
                    <td align="left" 	colspan="18" style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF;"><b>Time Total</b></td>
                    <td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;"><b>${toCurrency(timeTotal)}</b></td>
                </tr>

            </table>
	</#if>
</#if>

<!-- Expenses summary -->
<#if record.custbody_bbs_expenses_summary_json?length gt 0>

	<#assign summaryInfo = record.custbody_bbs_expenses_summary_json?eval>
