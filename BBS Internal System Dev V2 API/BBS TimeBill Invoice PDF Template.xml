	</tr>
	<tr>
		<td align="left" 	colspan="12" >&nbsp;</td>
		<td align="left" 	colspan="6"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF;"><b>Amount Outstanding</b></td>
		<td align="right" 	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-bottom: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;"><b>${toCurrency(record.amountremaining)}</b></td>
	</tr>
</table>


</body>

<!--Only generate the detail body section if we have any detail-->
<#if record.custbody_bbs_time_detail_json?length gt 0 || record.custbody_bbs_expenses_detail_json?length gt 0>

  <!--Detail body section-->
  <body header="nlheaderDetail" header-height="40px" footer="nlfooterDetail" footer-height="10px" padding="0.25in 0.25in 0.25in 0.25in" size="A4-LANDSCAPE">

    <!--Time detail section-->
  	<#if record.custbody_bbs_time_detail_json?length gt 0>

      <#assign detailInfo = record.custbody_bbs_time_detail_json?eval>
      <#if detailInfo?has_content>

          <#assign timeTotal = 0>
          <#assign timeHoursTotal = 0>
          <#assign timeSubTotal = 0>
          <#assign timeHoursSubTotal = 0>
          <#assign lastEmployee = "~~~~~~~~~~">

          <table style="width: 100%; margin-top: 10px;">
              <tr>
                  <th align="left" 	colspan="25" style="font-size: 10pt; padding-left: 6px;"><b>Time Details</b></th>
              </tr>
                  <tr>
                      <th align="left" 	colspan="2"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Date</th>
                      <th align="left"	colspan="3"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Employee</th>
                      <th align="left" 	colspan="5"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Item</th>
                      <th align="left" 	colspan="9"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Description</th>
                      <th align="center" 	colspan="2"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Hours</th>
                      <th align="center" 	colspan="2"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF;">Rate</th>
                      <th align="right" 	colspan="2"  style="padding: 5px 5px 5px 5px; border-left: 1px solid #6EBBFF; border-top: 1px solid #6EBBFF; border-right: 1px solid #6EBBFF;">Value</th>
                  </tr>

                  <#list detailInfo as time>

                      <#assign timeTotal += time.timeAmount>
					  <#assign timeHoursTotal += time.timeQuantity>

                      <#if time_index==0>
                          <#assign lastEmployee = time.timeEmployee>
                      </#if>

