<body header="nlheader" header-height="280px" footer="nlfooter" footer-height="80px" padding="0.5in 0.5in 0.5in 0.5in" size="A4">

<table class="itemtable" style="width: 100%; margin-top: 10px; border-right: 1px solid black;">
<#list record.item as item>
<#if item_index==0>
<thead>
	<tr>
	<th align="center" colspan="2" style="background-color: #000000; color: #ffffff;">Qty Ordered</th>
	<th align="center" colspan="4" style="background-color: #000000; color: #ffffff;">Product Code</th>
	<th align="center" colspan="6" style="background-color: #000000; color: #ffffff;">Product Description</th>
	<th align="right" colspan="2" style="background-color: #000000; color: #ffffff;">Unit Price</th>
	<th align="right" colspan="2" style="background-color: #000000; color: #ffffff;">Net Amt</th>
	<th align="right" colspan="2" style="background-color: #000000; color: #ffffff;">VAT Amt</th>
	</tr>
</thead>
</#if>
  <tr>
      <td align="center" colspan="2" style="border-bottom: 1px solid black; border-left: 1px solid black;">${item.quantity}</td>
      <td align="center" colspan="4" style="border-bottom: 1px solid black; border-left: 1px solid black;">${item.item}</td>
      <td align="center" colspan="6" style="border-bottom: 1px solid black; border-left: 1px solid black;">${item.description}</td>
      <td align="right" colspan="2" style="border-bottom: 1px solid black; border-left: 1px solid black;">${item.rate}</td>
      <td align="right" colspan="2" style="border-bottom: 1px solid black; border-left: 1px solid black;">${item.grossamt}</td>
      <td align="right" colspan="2" style="border-bottom: 1px solid black; border-left: 1px solid black;">${item.tax1amt}</td>
  </tr>
</#list>
</table>

<table class="total" style="width: 100%; margin-top: 10px;">
	<tr class="totalrow">
		<td colspan="4">&nbsp;</td>
		<td align="left" style="background-color: #000000; color: #ffffff; border-bottom: 1px solid white; font-weight: bold;">Total Net Amount</td>
		<td align="right" style="border-right: 1px solid black; border-top: 1px solid black;">${record.subtotal}</td>
	</tr>
	<tr class="totalrow">
		<td colspan="4">&nbsp;</td>
		<td align="left" style="background-color: #000000; color: #ffffff; border-bottom: 1px solid white; font-weight: bold;">Total VAT Amount</td>
		<td align="right" style="border-right: 1px solid black; border-top: 1px solid black;">${record.taxtotal}</td>
	</tr>
	<tr class="totalrow">
		<td colspan="4">&nbsp;</td>
		<td align="left" style="background-color: #000000; color: #ffffff; border-bottom: 1px solid white; font-weight: bold;">Order Total</td>
		<td align="right" style="border-right: 1px solid black; border-top: 1px solid black; border-bottom: 1px solid black;">${record.total}</td>
	</tr>
	<tr>
		<td>&nbsp;</td>
	</tr>
</table>

</body>
