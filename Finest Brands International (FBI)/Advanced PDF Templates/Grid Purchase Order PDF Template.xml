                  <td align="center" colspan="5" style="font-size: 8pt;">Company Registration No: 02545569. VAT Registration No: GB 927 5446 01</td>
                </tr>
              </table>
            </td>
            </tr>
            </table>

		<table class="footer" style="width: 100%; margin-top: 5px;">
          <tr>
			<td align="right"><pagenumber/> of <totalpages/></td>
		</tr>
        </table>
        </macro>
    </macrolist>

    <style type="text/css">
    	table {
            <!--font-family: sans-serif;-->
      		font-family: Calibri, Candara, Segoe, "Segoe UI", Optima, Arial, sans-serif;
            font-size: 9pt;
            table-layout: fixed;
        }
        th {
            font-weight: bold;
            font-size: 8pt;
            padding: 0px;
            <!--background-color: #e3e3e3;-->
            border-bottom: 1px solid black;
    		border-collapse: collapse;
            <!--color: #333333;-->
        }
        td {
            padding: 0px;
            vertical-align: top;
            font-size:10px;
        }
        b {
            font-weight: bold;
            color: #333333;
        }
        table.header td {
            padding: 0px;
            font-size: 10pt;
        }
        table.footer td {
            padding: 0;
            font-size: 6pt;
        }
        table.itemtable th {
            padding-bottom: 0px;
            padding-top: 0px;

        }
        table.body td {
            padding-top: 0px;
        }
        table.total {
            page-break-inside: avoid;
        }
      	table.message{
        	border: 1px solid #dddddd;
      	}
        tr.totalrow {
            <!--background-color: #e3e3e3;-->
            line-height: 200%;
        }
      	tr.messagerow{
        	font-size: 6pt;
      	}
        td.totalboxtop {
            font-size: 12pt;
            background-color: #e3e3e3;
        }
        td.addressheader {
            font-size: 10pt;
            padding-top: 0px;
            padding-bottom: 0px;
        }
        td.address {
            padding-top: 0;
          	font-size: 10pt;
        }
        td.totalboxmid {
            font-size: 28pt;
            padding-top: 20px;
            background-color: #e3e3e3;
        }
        td.totalcell {
          	border-bottom: 1px solid black;
    		border-collapse: collapse;
        }
      	td.message{
        	font-size: 8pt;
      	}
        td.totalboxbot {
            background-color: #e3e3e3;
            font-weight: bold;
        }
        span.title {
            font-size: 28pt;
        }
        span.number {
            font-size: 16pt;
        }
        span.itemname {
            font-weight: bold;
            line-height: 150%;
        }
        hr {
            width: 100%;
            color: #d3d3d3;
            background-color: #d3d3d3;
            height: 1px;
        }
</style>
</head>
  
<body header="nlheader" header-height="280px" footer="nlfooter" footer-height="60px" padding="0.5in 0.5in 0.5in 0.5in" size="A4">
  
<#assign summary_info = record.custbody_po_matrix_item_json?eval>

<table style="width: 100%; margin-top: 10px;">
	<tr>
		<td><b>Style:</b></td>
		<td colspan="2" rowspan="1"><b>${summary_info.outputArray[0].description}</b></td>
	</tr>
	<tr>
		<td><b>Order breakdown:</b></td>
	</tr>
</table>

<table style="width: 100%; margin-top: 10px; border-top: 1px solid black; border-left: 1px solid black;">
<#list summary_info.outputArray as info>
		<#if info_index==0>
          <thead>
            <tr>
              	<th colspan="3" style="border-right: 1px solid black; border-bottom: 1px solid black;">Description</th>
              
            	<#list info.quantitysize as qsh>
                  <th align="center" style="text-align: center; border-right: 1px solid black; border-bottom: 1px solid black;">${qsh.sizeText}</th>
                </#list>
              
              <th  align="center" style="border-right: 1px solid black; border-bottom: 1px solid black;">Total</th>
              
            </tr>
          </thead>
        </#if>
      
      	<tr>
      		<td colspan="3" style="border-right: 1px solid black; border-bottom: 1px solid black;">${info.colour}</td>
          
          	<#list info.quantitysize as qsd>
              	<#if qsd.quantity==0>
                  <td align="center" style="text-align: center; border-right: 1px solid black; border-bottom: 1px solid black;">&nbsp;</td>
                <#else>
                  <td align="center" style="text-align: center; border-right: 1px solid black; border-bottom: 1px solid black;">${qsd.quantity}</td>
                </#if> 
            </#list>
              
              <td  align="center" style="border-right: 1px solid black; border-bottom: 1px solid black;">${info.total}</td>
          
      	</tr>
</#list>
</table>

<table style="width: 100%; margin-top: 10px; border-top: 1px solid black; border-left: 1px solid black;">
  <thead>
    <tr>
      <th align="center" style="text-align: center; border-right: 1px solid black; border-bottom: 1px solid black;">Total Order Quantity</th>
      <th align="center" style="text-align: center; border-right: 1px solid black; border-bottom: 1px solid black;">Unit Price</th>
      <th align="center" style="text-align: center; border-right: 1px solid black; border-bottom: 1px solid black;">Total Order Value</th>
    </tr>
  </thead>
  <tr>
    <td align="center" style="text-align: center; border-right: 1px solid black; border-bottom: 1px solid black;">${summary_info.totalQuantity}</td>
    <td align="center" style="text-align: center; border-right: 1px solid black; border-bottom: 1px solid black;">${summary_info.unitPrice}</td>
    <td align="center" style="text-align: center; border-right: 1px solid black; border-bottom: 1px solid black;">${summary_info.totalAmount}</td>
    
  </tr>
</table>
  
<table style="width: 100%; margin-top: 20px;">
	<tr>
		<td width="25%"><b>Delivery into FBI:</b></td>
		<td>${record.duedate}</td>
		<td>Delivery by Appointment only</td>
	</tr>
	<tr>
		<td>&nbsp;</td>
	</tr>
	<tr>
		<td width="25%"><b>Production Specifications:</b></td>
		<td colspan="2" rowspan="1">${summary_info.outputArray[0].item_specification}</td>
	</tr>
	<tr>
		<td>&nbsp;</td>
	</tr>
	<tr>
		<td width="25%"><b>Trim:</b></td>
		<td colspan="2" rowspan="1">${summary_info.outputArray[0].item_trim}</td>
	</tr>
	<tr>
		<td>&nbsp;</td>
	</tr>
	<tr>
		<td width="25%"><b>Packaging and Barcodes:</b></td>
		<td colspan="2" rowspan="1">${summary_info.outputArray[0].item_packaging}</td>
	</tr>
	<tr>
		<td>&nbsp;</td>
	</tr>
	<tr>
		<td width="25%"><b>Outer Packaging:</b></td>
		<td colspan="2" rowspan="1">${summary_info.outputArray[0].item_outer_packaging}</td>
	</tr>
	<tr>
		<td>&nbsp;</td>
	</tr>
	<tr>
		<td width="25%"><b>Terms:</b></td>
		<td colspan="2" rowspan="1">${summary_info.outputArray[0].item_purchase_terms}</td>
	</tr>
	<tr>
		<td>&nbsp;</td>
	</tr>
	<tr>
		<td width="25%"><b>Authorised Signature:</b></td>
		<td>&nbsp;</td>
		<td>${record.trandate}</td>
	</tr>
</table>

  
</body>
</pdf>