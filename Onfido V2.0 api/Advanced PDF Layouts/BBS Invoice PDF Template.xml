<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
  	<link name="WorkSans" type="font" subtype="opentype" src="https://5554661-sb1.app.netsuite.com/core/media/media.nl?id=37338&amp;c=5554661_SB1&amp;h=8405786539e632199200&amp;_xt=.otf" src-bold="https://5554661-sb1.app.netsuite.com/core/media/media.nl?id=37339&amp;c=5554661_SB1&amp;h=51b66d9833053be2cd34&amp;_xt=.otf" src-italic="https://5554661-sb1.app.netsuite.com/core/media/media.nl?id=37340&amp;c=5554661_SB1&amp;h=19b16825b34ba5486a54&amp;_xt=.otf" src-bolditalic="https://5554661-sb1.app.netsuite.com/core/media/media.nl?id=37341&amp;c=5554661_SB1&amp;h=8a33438751bbe5e3f6f4&amp;_xt=.otf" bytes="2" />
  
  	<!--Default assigns-->
  	<#assign logoInfo>https://5554661-sb1.app.netsuite.com/core/media/media.nl?id=9544&amp;c=5554661_SB1&amp;h=3c1a74753ea9e8671a1c</#assign>
  	<#assign regInfo>Company Registration No: 07479524.  Registered Office: 3 Finsbury Avenue, 5th Floor, London, EC2M 2PA, United Kingdom.</#assign>
	<#assign emailInfo>accounts@onfido.com</#assign>
	<#assign telInfo>02081333628</#assign>
	<#assign vatInfo>VAT No: GB 159 2397 76</#assign>
  	<#assign ibanLabel>IBAN:</#assign>
  	<#assign signature>https://5554661-sb1.app.netsuite.com/core/media/media.nl?id=37336&amp;c=5554661_SB1&amp;h=d5b58d1ec190b9507c62</#assign>
      
      
   	<!--Reassign based on subsidiary-->
  	<#if subsidiary.id == "10"> <!--India-->
      <#assign ibanLabel>IFSC:</#assign>
      <#assign emailInfo>billing@onfido.com</#assign>
    </#if>
  
  	<#if subsidiary.id == "15"> <!--Singapore-->
    </#if>
  
  	<#if subsidiary.id == "6"> <!--France-->
    </#if>
  
  	<#if subsidiary.id == "8"> <!--Portugal-->
    </#if>
  
  	<#if subsidiary.id == "5"> <!--Uk-->
    </#if>
  
  	<#if subsidiary.id == "9"> <!--USA-->
    </#if>
  
  

  
    <macrolist>
        <macro id="nlheader">
			<table class="header" style="width: 100%;">
    			<tr>
					<td style="font-size:22px; padding-top: 55px;">TAX INVOICE</td>
					<td align="right">&nbsp;</td>
                  	<td align="right"><img src="${logoInfo}" style="float: right; width:250px; height:130px;"/></td>
    			</tr>
          	</table>
          
			<table class="header" style="width: 100%; margin-top: 10px;">
        		<tr>
        			<td rowspan="7" colspan="2" style="font-size:12pt">${record.billaddress}<br/><br/>VAT Number: ${record.entitytaxregnum}</td>
        			<td>&nbsp;</td>
        			<td colspan="2" style="font-size:12pt"><b>Invoice Date</b></td>
        			<td rowspan="7" colspan="2" style="font-size:12pt">${subsidiary.mainaddress_text}<br/>${vatInfo}<br/>billing@onfido.com</td>
        		</tr>
        		<tr>
        			<td>&nbsp;</td>
        			<td colspan="2" style="font-size:12pt">${record.trandate?string.long}</td>
        		</tr>
        		<tr>
        			<td>&nbsp;</td>
        			<td colspan="2" style="font-size:12pt"><b>Invoice Number</b></td>
        		</tr>        		
        		<tr>
        			<td>&nbsp;</td>
        			<td colspan="2" style="font-size:12pt">${record.tranid}</td>
        		</tr>        		
        		
              	<tr>
        			<td>&nbsp;</td>
        			<td colspan="2" style="font-size:12pt"><b>Period</b></td>
        		</tr>
        		<tr>
        			<td>&nbsp;</td>
        			<td colspan="2" style="font-size:12pt">${record.postingperiod}</td>
        		</tr>
              	<tr>
        			<td>&nbsp;</td>
        			<td colspan="2" style="font-size:12pt"><b>&nbsp;</b></td>
        		</tr>
              	<tr>
        			<td colspan="2">&nbsp;</td>
        			<td>&nbsp;</td>
        			<td colspan="2" style="font-size:12pt">&nbsp;</td>
        			<td colspan="2" style="font-size:12pt">&nbsp;</td>
        		</tr>
              	    		
        		
          	</table>
        </macro>

        <macro id="nlfooter">

			<table class="footer" style="width: 100%;">
          		<tr>
                  	<td colspan="8" style="font-size: 6pt;">${regInfo}</td>
					<td align="right" style="font-size: 6pt;">Page <pagenumber/> of <totalpages/></td>
				</tr>
        	</table>
        </macro>
    </macrolist>

    <style type="text/css">* {
			font-family: WorkSans;
		}
		table {
			font-size: 9pt;
			table-layout: fixed;
		}
        th {
            font-weight: bold;
            font-size: 8pt;
            vertical-align: middle;
            padding: 5px 6px 3px;
          	border-bottom: 1px solid black;
            <!--background-color: #e3e3e3;-->
            color: #333333;
        }
        td {
            padding: 2px 4px;
        }
		td p { align:left }
        b {
            font-weight: bold;
            color: #333333;
        }
        table.header td {
            padding: 0px;
            font-size: 10pt;
        }
        table.footer td {
            padding: 0px;
            font-size: 8pt;
        }
        table.itemtable th {
            padding-bottom: 10px;
            padding-top: 10px;
        }
        table.body td {
            padding-top: 2px;
        }
        table.total {
            page-break-inside: avoid;
        }
        tr.totalrow {
            background-color: #e3e3e3;
            line-height: 200%;
        }
        td.totalboxtop {
            font-size: 12pt;
            background-color: #e3e3e3;
        }
        td.addressheader {
            font-size: 8pt;
            padding-top: 6px;
            padding-bottom: 2px;
        }
        td.address {
            padding-top: 0px;
        }
        td.totalboxmid {
            font-size: 28pt;
            padding-top: 20px;
            background-color: #e3e3e3;
        }
        td.totalboxbot {
            background-color: #e3e3e3;
            font-weight: bold;
        }
      td.itemtable {
        border-bottom: 1px solid lightgrey;
      	}
        span.title {
            font-size: 28pt;
        }
        span.number {
            font-size: 16pt;
        }
        span.itemname {
            font-weight: bold;
            line-height: 150%;
        }
        hr {
            width: 100%;
            color: #d3d3d3;
            background-color: #d3d3d3;
            height: 1px;
        }
</style>
</head>
<body header="nlheader" header-height="300px" footer="nlfooter" footer-height="10px" padding="0.25in 0.25in 0.25in 0.25in" size="A4">
  
<#if subsidiary.id == "10"> <!--India-->
  <table class="header" style="width: 30%;">
    <tr>
      <td>Place of supply:</td>
      <td>${record.custbody_in_gst_pos}</td>
    </tr>
    <tr>
      <td>State:</td>
      <td>${record.custbody_in_gst_pos?keep_after("-")}</td>
    </tr>
    <tr>
      <td>Code:</td>
      <td>${record.custbody_in_gst_pos?keep_before("-")}</td>
    </tr>
  </table>
</#if>
  

<#assign summary_info = record.custbody_bbs_json_summary?eval>
			
<#if summary_info?has_content>
  <table class="itemtable" style="width: 100%;">
      <#list summary_info as item>
          <#if item_index==0>
              <thead>
                  <tr>
                      <th colspan="12" style="font-size: 12pt;">Description</th>
                      <th align="center" colspan="3" style="font-size: 12pt;">Quantity</th>
                      <th align="right" colspan="4" style="font-size: 12pt;">Unit Price</th>
                      <th align="right" colspan="4" style="padding: 10px 6px; white-space: nowrap; font-size: 12pt;">Tax Rate</th>
                      <th align="right" colspan="4" style="font-size: 12pt;">Amount</th>
                  </tr>
              </thead>
          </#if>
          <tr>
              <td class="itemtable" colspan="12" style="font-size: 12pt;">${item.salesDescription}</td>
              <td class="itemtable" align="center" colspan="3" line-height="150%" style="font-size: 12pt;">${item.quantity?string["#,##0.00"]}</td>
              <td class="itemtable" align="right" colspan="4" style="font-size: 12pt;">${item.symbol}${item.unitPrice?string["#,##0.00"]}</td>
              <td class="itemtable" align="right" colspan="4" style="font-size: 12pt;">${item.vatRate}</td>
              <td class="itemtable" align="right" colspan="4" style="font-size: 12pt;">${item.symbol}${item.amount?string["#,##0.00"]}</td>
          </tr>
      </#list>
  </table>
</#if>


<table class="total" style="width: 100%; margin-top: 10px;">
	<tr>
      <td colspan="4">&nbsp;</td>
      <td align="right" style="font-size: 12pt;"><b>${record.subtotal@label}</b></td>
      <td align="right" style="font-size: 12pt;">${record.subtotal}</td>
      </tr>
      <tr>
      <td colspan="4">&nbsp;</td>
      <td align="right" style="font-size: 12pt;"><b>${record.taxtotal@label}</b></td>
      <td align="right" style="font-size: 12pt;">${record.taxtotal}</td>
      </tr>
      <tr>
      <td colspan="4">&nbsp;</td>
      <td align="right" style="border-top: 1px solid black; font-size: 12pt;"><b>${record.total@label}</b></td>
      <td align="right" style="border-top: 1px solid black; font-size: 12pt;"><b>${record.total}</b></td>
	</tr>
</table>

<#assign dueDate="">
<#if record.duedate?length gt 0>
	<#assign dueDate=record.duedate?string.long>
</#if>

<table class="header" style="width: 100%; margin-top: 20px; page-break-inside: avoid;">
	<tr>
      <td colspan="4" style="font-size: 12pt; font-weight: bold;">Due Date: ${dueDate}</td>
    </tr>
</table>
  
<table class="header" style="width: 100%; margin-top: 20px; page-break-inside: avoid;">
  	<tr>
      <td style="font-size: 12pt;">Bank:</td>
      <td style="font-size: 12pt;">${record.custbody_bbs_bank_name}</td>
      <#if subsidiary.id == "10"> <!--India-->
      	<td colspan="1" rowspan="5" align="center"><img src="${signature}" style="float: right; width:250px; height:105px;"/></td>
      <#else>
        <td colspan="1" rowspan="5" align="center" style="font-size: 12pt;">&nbsp;</td>
      </#if>
    </tr>
    <tr>
      <td style="font-size: 12pt;">Sort Code:</td>
      <td style="font-size: 12pt;">${record.custbody_bbs_bank_sort_code}</td>
    </tr>
    <tr>
      <td style="font-size: 12pt;">Account Number:</td>
      <td style="font-size: 12pt;">${record.custbody_bbs_bank_account_number}</td>
    </tr>
    <tr>
      <td style="font-size: 12pt;">${ibanLabel}</td>
      <td style="font-size: 12pt;">${record.custbody_bbs_bank_iban}</td>
    </tr>
    <tr>
      <td style="font-size: 12pt;">SWIFT:</td>
      <td style="font-size: 12pt;">${record.custbody_bbs_bank_bic}</td>
    </tr>
</table>
  
<#if subsidiary.id == "10"> <!--India-->
  <table class="header" style="width: 30%; margin-top: 20px; page-break-inside: avoid;">
      <tr>
        <td colspan="2"><b>Billing Information</b></td>
      </tr>
      <tr>
        <td>GSTIN:</td>
        <td>06AACCO6506A1ZZ</td>
      </tr>
      <tr>
        <td>PAN:</td>
        <td>${subsidiary.custrecord_in_permanent_account_number}</td>
      </tr>
  </table>
</#if>
  
</body>
</pdf>